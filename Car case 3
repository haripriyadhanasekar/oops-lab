package lab;

public class Car {
	    String model;
	    String color;
	    public Car(String model, String color) {
	        this.model = model;
	        this.color = color;
	    }
	    public void start() {
	        System.out.println("Car started");
	    }
	    public void stop() {
	        System.out.println("Car stopped");
	    }
	    public static void main(String[] args) {
	        Car myCar = new Car(" ", "Black");
	        myCar.start();
	    }
	}

